@model HomeViewModel

@inject LayoutService service
@{
    double totalArea = 0;
}


<!-- SLIDER AREA START (slider-3) -->
<div class="ltn__slider-area ltn__slider-3  section-bg-1">
    <div class="ltn__slide-one-active slick-slide-arrow-1 slick-slide-dots-1">

        @foreach (var slide in Model.Sliders)
        {
            <div class="ltn__slide-item ltn__slide-item-2 ltn__slide-item-3-normal ltn__slide-item-3">
                <div class="ltn__slide-item-inner @(slide.Order%2 == 0?"text-right text-end":"")">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 align-self-center">
                                <div class="slide-item-info">
                                    <div class="slide-item-info-inner ltn__slide-animation">
                                        @if (slide.Order % 2 == 1)
                                        {
                                            <div class="slide-video mb-50 d-none">
                                                <a class="ltn__video-icon-2 ltn__video-icon-2-border" href="https://www.youtube.com/embed/tlThdr3O5Qo" data-rel="lightcase:myCollection">
                                                    <i class="fa fa-play"></i>
                                                </a>
                                            </div>
                                        }
                                        <h6 class="slide-sub-title white-color--- animated"><span><i class="fas fa-home"></i></span> @slide.MiniTitle</h6>
                                        <h1 class="slide-title animated ">@slide.Title1<br> @slide.Title2</h1>
                                        <div class="slide-brief animated">
                                            <p>@slide.Desc</p>
                                        </div>
                                        <div class="btn-wrapper animated">
                                            <a asp-action="register" asp-controller="account" class="theme-btn-1 btn btn-effect-1">Qeydiyyatdan keçin</a>
                                            <a class="ltn__video-play-btn bg-white" href="https://www.youtube.com/embed/HnbMYzdjuBs?autoplay=1&amp;showinfo=0" data-rel="lightcase">
                                                <i class="icon-play  ltn__secondary-color"></i>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="slide-item-img @(slide.Order %2 ==0?"slide-img-left":"")">
                                    <img src="~/uploads/slider/@slide.Image" alt="#">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }

    </div>
</div>
<!-- SLIDER AREA END -->


<!-- CAR DEALER FORM AREA START -->
