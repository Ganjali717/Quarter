<!-- PRODUCT SLIDER AREA START -->
<div class="ltn__product-slider-area ltn__product-gutter pt-115 pb-90 plr--7">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title-area ltn__section-title-2--- text-center">
                    <h6 class="section-subtitle section-subtitle-2 ltn__secondary-color">Evlər</h6>
                    <h1 class="section-title">Xüsusiyyətli evlər</h1>
                </div>
            </div>
        </div>
        <div class="row ltn__product-slider-item-four-active-full-width slick-arrow-1">

            @foreach (var item in Model.Houses)
            {
                if (item.IsFeatured == true)
                {
                    <div class="col-lg-12">
                        <div class="ltn__product-item ltn__product-item-4 text-center---">
                            <div class="product-img">
                                <a asp-action="detail" asp-controller="shop" asp-route-id="@item.Id"><img src="~/uploads/house/@item.HouseImages.FirstOrDefault(x => x.PosterStatus ==true)?.Image" alt="#"></a>
                                <div class="product-badge">
                                    <ul>
                                        <li class="sale-badge bg-green">@item.HouseStatus.StatusName</li>
                                    </ul>
                                </div>
                                <div class="product-img-location-gallery">
                                    <div class="product-img-location">
                                        <ul>
                                            <li>
                                                <a href="#"><i class="flaticon-pin"></i> @item.Location, @item.City.Name</a>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="product-img-gallery">
                                        <ul>
                                            <li>
                                                <a asp-action="detail" asp-controller="shop" asp-route-id="@item.Id"><i class="fas fa-camera"></i> @item.HouseImages.Count</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="product-info">
                                <div class="product-price">
                                    <span>@item.SalePrice AZN<label>@(item.HouseStatus.Id == 2 ?"/Aylıq":"")</label></span>
                                </div>
                                <h2 class="product-title"><a asp-action="detail" asp-controller="shop" asp-route-id="@item.Id">@item.HouseType.Name</a></h2>
                                <div class="product-description">
                                    <p>
                                        @item.Desc
                                    </p>
                                </div>
                                <ul class="ltn__list-item-2 ltn__list-item-2-before">
                                    <li>
                                        <span>@item.Beds </span><i class="flaticon-bed"></i>
                                        Yataq otağı
                                    </li>
                                    <li>
                                        <span>@item.Baths </span><i class="flaticon-clean"></i>
                                      Hamam
                                    </li>
                                    <li>
                                        <span>@item.Area</span> <i class="flaticon-square-shape-design-interface-tool-symbol"></i>
                                        kv.m
                                    </li>
                                </ul>
                            </div>
                            <div class="product-info-bottom">
                                <div class="real-estate-agent">
                                    <div class="agent-img">
                                        <a asp-action="detail" asp-controller="team" asp-route-id="@item.Team.Id"><img src="~/uploads/team/@item.Team.Image" alt="#"></a>
                                    </div>
                                    <div class="agent-brief">
                                        <h6><a asp-action="detail" asp-controller="team" asp-route-id="@item.Team.Id">@item.Team.Fullname</a></h6>
                                        <small>Makler</small>
                                    </div>
                                </div>
                                <div class="product-hover-action">
                                    <ul>
                                        <li>
                                            <a href="#" title="Quick View" data-bs-toggle="modal" data-bs-target="#quick_view_modal" data-id="@item.Id" class="show-modal-house">
                                                <i class="flaticon-expand"></i>
                                            </a>
                                        </li>
                                        <li>
                                            <a asp-action="addwishlist" asp-controller="shop" asp-route-id="@item.Id" data-id="@item.Id" title="Wishlist" data-bs-toggle="modal" data-bs-target="#liton_wishlist_modal" class="add-wish-btn">
                                                <i class="flaticon-heart-1"></i>
                                            </a>
                                        </li>
                                        <li>
                                            <a asp-action="detail" asp-controller="shop" asp-route-id="@item.Id" title="Product Details">
                                                <i class="flaticon-add"></i>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            }
        </div>
    </div>
</div>
<!-- PRODUCT SLIDER AREA END -->


